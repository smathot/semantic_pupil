define form_base form_rating_brightness
	set timeout "infinite"
	set spacing 10
	set rows "1;1"
	set only_render "no"
	set margins "50;150;50;150"
	set description "A generic form plug-in"
	set cols 1
	set _theme "gray_large"
	widget 0 0 1 1 image path="[word].png"
	widget 0 1 1 1 rating_scale click_accepts=yes nodes="Très lumineux;Lumineux;Neutre;Foncé;Très foncé" var=rating


