define loop counterbalance_loop
	set skip 0
	set repeat 1
	set order "random"
	set offset "no"
	set item "counterbalance_sequence"
	set description "Repeatedly runs another item"
	set cycles 2
	set column_order "rating_type"
	set break_if "never"
	setcycle 0 rating_type "[rating_type_1]"
	setcycle 1 rating_type "[rating_type_2]"
	run counterbalance_sequence

define inline_script counterbalance_script
	set description "Executes Python code"
	set _run ""
	___prepare__
	if var.subject_parity == 'odd':
		var.rating_type_1 = 'emotion'
		var.rating_type_2 = 'brightness'
	else:
		var.rating_type_1 = 'brightness'
		var.rating_type_2 = 'emotion'
	__end__

define sequence counterbalance_sequence
	set flush_keyboard "yes"
	set description "Runs a number of items in sequence"
	run form_instructions_emotion "[rating_type] = emotion"
	run form_instructions_brightness "[rating_type] = brightness"
	run blank "always"
	run block_loop "always"
	run blank "always"

define sequence experiment
	set flush_keyboard "yes"
	set description "The main sequence of the experiment"
	run counterbalance_script "always"
	run form_instructions "always"
	run blank "always"
	run form_instructions_2 "always"
	run blank "always"
	run form_instructions_3 "always"
	run blank "always"
	run counterbalance_loop "always"
	run form_text_display "always"

define form_text_display form_instructions
	set timeout "infinite"
	set spacing 10
	set rows "1;4;1"
	set only_render "no"
	set ok_text "Je comprends"
	set margins "50;50;50;50"
	set form_title "Instructions"
	__form_text__
	Dans cette expérience, vous verrez des mots. Pour chaque mot, indiquez :
	
	- Si le mot vous donne un sentiment de <b>luminosité</b> ou <b>d'obscurité</b>
	- Si le mot vous évoque une émotion <b>négative</b> ou <b>positive</b>
	__end__
	set description "A simple text display form"
	set cols "1;1;1"
	set _theme "gray"
	widget 0 0 3 1 label text="[form_title]"
	widget 0 1 3 1 label center=no text="[form_text]"
	widget 1 2 1 1 button text="[ok_text]"


define form_text_display form_instructions_2
	set timeout "infinite"
	set spacing 10
	set rows "1;4;1"
	set only_render "no"
	set ok_text "Je comprends"
	set margins "50;50;50;50"
	set form_title "Exemples"
	__form_text__
	- Le mot <b>"soleil"</b> est :
	   - Trés lumineux
	   - Positif
	   
	- Le mot <b>"fourchette"</b> est :
	   - Ni lumineux, ni foncé, donc la luminosité de ce mot est neutre
	   - Ni positif, ni negatif, donc l'émotionalité de ce mot est neutre
	   
	- Le mot <b>"mort"</b> est :
	   - Ni lumineux, ni foncé, donc la luminosité de ce mot est neutre
	   - Trés négatif
	__end__
	set description "A simple text display form"
	set cols "1;1;1"
	set _theme "gray"
	widget 0 0 3 1 label text="[form_title]"
	widget 0 1 3 1 label center=no text="[form_text]"
	widget 1 2 1 1 button text="[ok_text]"


