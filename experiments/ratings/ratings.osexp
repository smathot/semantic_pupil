define logger logger
	set use_quotes "yes"
	set ignore_missing "yes"
	set description "Logs experimental data"
	set auto_log "no"
	log word
	log type
	log count_trial_sequence
	log rating
	log rating_type
	log subject_nr
	log subject_parity

define sequence trial_sequence
	set flush_keyboard "yes"
	set description "A single trial"
	run form_rating_brightness "[rating_type] = brightness"
	run form_rating_emotion "[rating_type] = emotion"
	run blank "always"
	run logger "always"

